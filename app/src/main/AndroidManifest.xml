<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.appsqllite">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".RegistroUsuarioActivity"
            android:label="Registro usuarios"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".ConsultarUsuarioActivity"
            android:label="Consultar usuarios"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".ConsultarComboActivity"
            android:label="Consultar lista de Usuarios"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
            android:name=".ConsultarListViewActivity"
            android:label="Consultar lista de Usuarios en listView"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
    </application>

</manifest>